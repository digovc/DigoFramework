<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:DigoFramework_WPF_Control_Panel="clr-namespace:DigoFramework.WPF.Control.Panel">

    <Style TargetType="{x:Type DigoFramework_WPF_Control_Panel:PanelRelevoDock}" BasedOn="{StaticResource {x:Type DigoFramework_WPF_Control_Panel:DockPanelMain}}">
        <Setter Property="Background">
            <Setter.Value>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="White" Offset="0" />
                    <GradientStop Color="#FFF0F0F0" Offset="1" />
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>